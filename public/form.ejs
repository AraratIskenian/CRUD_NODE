<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>

    <table>
        <tr>
            <td>Name</td>
            <td>Price</td>
            <td>Update</td>
            <td>Delete</td>
        </tr>
         <% obj.forEach(function(justOne) { %>
        <tr>
            <td><%= justOne.name %></td>
            <td><%= justOne.price %></td>
            <td><a class="update" href="update/<%= justOne.uuid %>">Update</a></td>
            <td><a class="delete" href="delete/<%= justOne.uuid %>">Delete</a></td>
        </tr>
    <% }); %>
    </table>
    <form action="/addName" method="post">
        <input id="PrName" class="inp" type="text" required name="name" placeholder="Name"><br><br>
        <input id="PrPrice" class="inp" type="number" name="price" placeholder="Price"><br><br>
        <input id="PrImage" class="inp" type="text" name="image" placeholder="Image"><br><br>
        <input id="PrUUID" class="inp" type="number" name="uuid" placeholder="UUID"><br><br>
        <input id="PrDes" class="inp" type="text" name="description" placeholder="Description"><br><br>
        <input type="submit" id="submit" onclick="getVal()" >
    </form>

    <script src="script.js"></script>
</body>
</html>
