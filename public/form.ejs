<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>

    <ul>
        <ul>
            <% obj.forEach(function(justOne) { %>
            <li>
            Id - <strong><%= justOne.theaterId %></strong> <br>
            City - <strong><%= justOne.location.address.city %></strong> /
            State - <strong><%= justOne.location.address.state %></strong> /
            Zip code - <strong><%= justOne.location.address.zipcode %></strong> /
            Street - <strong><%= justOne.location.address.street1 %></strong> <br> <br>
            </li>
            <% }); %>
        </ul>
    <br>
    <form action="/addName" method="post">
        <input type="text" onkeydown="sendData()" id="name" required name="name" class="inp" placeholder="Please type your name">
            <br>
            <br>
        <input type="email" class="inp" name="email" id="email" placeholder="Please type your email">
            <br>
            <br>
        <input type="password" class="inp" name="password" id="password" placeholder="Please type your password">
            <br>
            <br>
        <input type="submit" onclick="sendData()" class="inp">
    </form>

    <script src="script.js"></script>
</body>
</html>